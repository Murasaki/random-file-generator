{
  "name": "murasaki-random-file-generator",
  "version": "1.4.0",
  "description": "A simple file generator that uses brute force to create files of a certain size.",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "mocha -r ts-node/register 'src/**/*.tests.ts'",
    "test:watch": "mocha -r ts-node/register ./**/*.tests.ts --watch --watch-files ./**/*.tests.ts",
    "test:coverage": "nyc npm run test",
    "copy:windows": "cpx ../../../lib/windows/SDK/glasswall.classic.dll ./dist/lib/windows/SDK/ -u",
    "run:example-usage": "npm run build && node .//dist//example-usage//generate-test-set.js",
    "run:example-usage-azure-function": "npm run build && node .//dist//example-usage//generate-test-set-with-azure-function.js",
    "packnpublish": "npm run build && npm publish"
  },
  "keywords": [
    "typescript",
    "tsc",
    "random",
    "file",
    "generator",
    "images",
    "test data"
  ],
  "dependencies": {
    "@azure/functions": "^1.2.2",
    "@types/node": "^14.0.13",
    "node-fetch": "^2.6.0",
    "sharp": "^0.25.4",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node-fetch": "^2.5.7",
    "@types/sharp": "^0.25.0",
    "@types/sinon": "^9.0.4",
    "@types/yargs": "^15.0.5",
    "chai": "^4.2.0",
    "eslint-plugin-react": "^7.19.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "dist/src"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.js"
}
